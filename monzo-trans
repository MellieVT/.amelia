#!/usr/bin/env bash
pink="\033[01;31m"
cyan="\033[00;36m"
whte="\033[01;37m"

echo -e ""
echo -e "${cyan}      ███               ███     "
echo -e "${cyan}    ███████           ███████   "
echo -e "${pink}  ████████████     ████████████ "
echo -e "${pink}  █████████████████████████████ "
echo -e "${whte}  ███████  ███████████  ███████ "
echo -e "${whte}  ███████    ███████    ███████ ${whte}   ████     █████   ████████   ████"
echo -e "${pink}  ███████      ███      ███████ ${whte} ███  ███  ███  ███    ████  ███  ███"
echo -e "${pink}  ███████               ███████ ${whte} ███  ███  ███  ███   ████   ███  ███"
echo -e "${cyan}    █████               █████   ${whte} ███  ███  ███  ███  ████    ███  ███"
echo -e "${cyan}      ███               ███     ${whte}   ████    ███  ███ ████████   ████"
echo -e "\e[0m"
